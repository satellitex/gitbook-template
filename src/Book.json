{
    "plugins": [
        "custom-favicon",
        "insert-logo",
        "expand-active-chapter",
        "navigator",
        "uml",
        "prism",
        "image-captions",
        "algolia", "lunr",
        "github", "github-buttons",
        "atoc", "toc",
        "codeblock-filename", 
        "mathjax",
        "hints",
        "page-ref",
        "copy-code-button",
        "codetabs"
    ],
    "pluginsConfig": {
        "favicon": "./.gitbook/assets/images/icon/favicon.ico",
        "insert-logo": {
            "url": "../.gitbook/assets/images/mitsuhisaT_logo_L.png",
            "style": "background: none;"
        },
        "sharing": {
            "facebook": false,
            "twitter": false,
            "google": true,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
            "facebook", "twitter", "google", "weibo", "instapaper", "vk"
            ]
        },
        "uml": {
            "charset": "utf-8",
            "format": "svg"
        },
        "algolia": {
            "index": "algolia-index-name",
            "applicationID": "algolia-application-id",
            "publicKey": "algolia-search-only-api-key",
            "freeAccount": "true"
        },
        "github": {
            "url": "https://github.com/stakedtechnologies/substrate.jp"
        }
    },
    "language": "ja"
}